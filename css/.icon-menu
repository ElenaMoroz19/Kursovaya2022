.icon-menu{ display: none;}
@media(max-width: 767.98px){
    .icon-menu{
    display: flex; position: relative;
    flex: 0 0 30px;  height: 18px;
    display: flex;  flex-direction: column;
    justify-content: space-between; margin: 0px 0px 0px 20px;}
    .icon-menu::before,
    .icon-menu::after { content: ""; background-color: #000;height: 2px;}
    .icon-menu span{ height: 2px;  background-color: #000;}
}
